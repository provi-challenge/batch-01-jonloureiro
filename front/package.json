{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "daisyui": "^2.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-imask": "^6.4.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
